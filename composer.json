    "type": "library",
    "keywords": [
        "Laravel",
        "Laravel Socialite",
        "Telegram",
        "OAuth"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adnan RIHAN",
            "email": "adnan@rihan.fr"
        },
        
        {
            "name": "Anton Kovalenko",
            "email": "tonylovepony@gmail.com"
        }
    ],
    "support": {
